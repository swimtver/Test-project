<?xml version="1.0"?>

<configuration>
	<connectionStrings>    
	<add name="photoBase" connectionString="Data Source=.\SQLEXPRESS;AttachDbFilename=|DataDirectory|\PhotoBase.mdf;Integrated Security=True;User Instance=True"  providerName="System.Data.SqlClient"/>
	</connectionStrings>

	<system.web>
		<compilation debug="true" targetFramework="4.0" />
	</system.web>

	<system.webServer>
		 <modules runAllManagedModulesForAllRequests="true"/>
	</system.webServer>
</configuration>
